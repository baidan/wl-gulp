    "karma-phantomjs-launcher": "^1.0.0",
    "lodash": "^4.13.1",
    "merge-stream": "^1.0.0",
    "minimist": "^1.2.0",
    "mocha": "^2.5.3",
    "node-msg": "^1.2.3",
    "node-sass": "^3.7.0",
    "phantomjs": "^2.1.7+deprecated",
    "rimraf": "^2.5.2",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0",
    "wiredep": "^4.0.0",
    "wrench": "^1.5.9"
  },
  "scripts": {
    "clean-deps": "rimraf node_modules"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "//": "以下是自定义配置",
  "config": {
    "//": "打包js,css资源",
    "host": "127.0.0.1",
    "port": "3000",
    "baseUrl": "./",
    "directories": {
      "source": "src",
      "//": "data数据存放处",
      "templter": "templter",
      "destination": "build",
      "temporary": "tmp",
      "modules": "_modules",
      "layouts": "_layouts",
      "images": "_images",
      "sprite": "_sprite",
      "styles": "_styles",
      "scripts": "_scripts",
      "data": "_data"
    },
    "wl": {
      "css": "Content",
      "//": "public--网兰公用资源",
      "js": "Scripts",
      "img": "Images",
      "pname": "pname",
      "modules": [
        "module",
        "activity",
        "partner",
        "public"
      ]
    },
    "entries": {
      "js": "main**.{js,coffee}",
      "css": "main**.{sass,scss,less,styl,css}"
    }
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browser": {
    "slick-carousel": "./node_modules/slick-carousel/slick/slick.js"
  },
  "browserify-shim": {
    "slick-carousel": {
      "exports": null,
      "depends": "jquery:$"
    }
  }
}