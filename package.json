    "imagemin-svgo": "^5.1.0",
    "jade": "^1.11.0",
    "js-yaml": "^3.6.1",
    "karma": "^0.13.22",
    "karma-browserify": "^5.0.5",
    "karma-chai": "^0.1.0",
