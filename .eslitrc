{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "globals": {
    "DocumentFragment": true,
    "expect": true,
    "it": true,
    "describe": true,
    "beforeEach": true,
    "runs": true,
    "spyOn": true,
    "spyOnEvent": true,
    "waitsFor": true,
    "expect": true,
    "afterEach": true,
    jQuery: true
  },
  "rules": {
    "camelcase": [2, { "properties": "always" }],
    "comma-dangle": [2, "never"],
    "semi": [2, "always"],
    "quotes": [2, "single"],
    "strict": [2, "global"]
},
}
